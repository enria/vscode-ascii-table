{
	"name": "ascii-table",
	"displayName": "ASCII Table",
	"description": "Show ASCII code table in palette",
	"version": "1.0.0",
	"publisher": "zhangyd",
	"repository": {
		"type": "git",
		"url": "https://github.com/enria/vscode-ascii-table.git"
	},
	"icon": "ascii.png",
	"engines": {
		"vscode": "^1.42.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.allChars",
		"onCommand:extension.printableChars"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.allChars",
				"title": "ASCII: All characters"
			},
			{
				"command": "extension.printableChars",
				"title": "ASCII: Printable characters"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.42.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.18.0",
		"@typescript-eslint/eslint-plugin": "^2.18.0",
		"glob": "^7.1.6",
		"mocha": "^7.0.1",
		"typescript": "^3.7.5",
		"vscode-test": "^1.3.0"
	}
}